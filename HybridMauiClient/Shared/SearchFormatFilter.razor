@using MtGDomain.DTO;
@using static HybridMauiClient.Shared.MagicCardSearch;
<label for="FormatId">Format: </label>
<select @bind="Model.ChoosenValue" id="FormatId">
    <option value="">--Please choose an option--</option>
    @for (int i = 0; i < Model.Formats.Length; i++)
    {
        <option value="@Model.Formats[i]">@Model.Formats[i]</option>
    }
</select>

@code {
    [Parameter]
    public MtGFormatFilter Model { get; set; }
}
